{
    "id": "fvttnineshadows",
    "title": "Nine Shadows",
    "description": "A fantasy RPG system based on the Enneagram of Personality.",
    "version": "2025.5.18.0",
    "url": "https://github.com/bmurr3/fvttnineshadows",
    "compatibility": {
        "minimum": 13
    },
    "authors": [{
        "name": "Will Waller",
        "email": "williammurray98@gmail.com",
        "discord": "wwally2022"
    }],
    "esmodules": ["fvttnineshadows.mjs"],
    "styles": ["styles/default-styles.css"],
    "packs": [],
    "languages": [{
        "lang": "en",
        "name": "English",
        "path": "lang/en.json"
    },
    {
        "lang": "fr",
        "name": "French",
        "path": "lang/fr.json"
    }],
    "documentTypes": {
        "Actor": {
            "pc": {
                "htmlFields": ["details.biography.value", "details.biography.public"]
            },
            "npc": {
                "htmlFields": ["details.biography.value", "details.biography.public"]
            }
        }
    },
    "flags": {
        "hotReload": {
            "extensions": ["css", "hbs", "json"],
            "paths": ["lang"]
        }
    }
}